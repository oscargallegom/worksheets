        <%= 'ac' unless @farm.decision_ag == 'N/A' %>
      </li>
                              <li>
        <div class='extrawide'>Acres of prescribed grazing:</div>
        <%= number_with_precision(@farm.prescribed_grazing, :precision => 2, :separator => '.', :delimiter => ',') unless @farm.prescribed_grazing == 'N/A' %>

            <li>
        <div class='extrawide'>Acres of soil and water management plans:</div>
        <div class='narrow'><%= number_with_precision(@farm.grass_buffered_area, :precision => 2, :separator => '.', :delimiter => ',') %>
        <%= 'ac' unless @farm.grass_buffered_area == 'N/A' %></div> <div class='narrow'><%= number_with_precision(@farm.grass_buffered_area_future, :precision => 2, :separator => '.', :delimiter => ',') %>
        <%= 'ac' unless @farm.grass_buffered_area_future == 'N/A' %></div>
      </li>
      </li>
                  <li>
        <div class='extrawide'>Acres treated by water control structures:</div>
        <%= number_with_precision(@farm.water_control_structures, :precision => 2, :separator => '.', :delimiter => ',') unless @farm.water_control_structures == 'N/A' %>
        <%= 'N/A' if @farm.water_control_structures == 'N/A' %>
        <%= 'ac' unless @farm.water_control_structures == 'N/A' %>
      </li>
                        <li>
        <div class='extrawide'>Acres of sorbing materials in ag ditches:</div>
        <%= number_with_precision(@farm.sorbing_materials, :precision => 2, :separator => '.', :delimiter => ',') unless @farm.sorbing_materials == 'N/A' %>
        <%= 'N/A' if @farm.sorbing_materials == 'N/A' %>
        <%= 'ac' unless @farm.sorbing_materials == 'N/A' %>
      </li>
                        <li>
        <div class='extrawide'>Acres of decision/precision agriculture:</div>
        <%= number_with_precision(@farm.decision_ag, :precision => 2, :separator => '.', :delimiter => ',') unless @farm.decision_ag == 'N/A' %>
